<?xml version="1.0" encoding="UTF-8" ?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
    version="1.0">
    <description>DataNucleus JPA ORM mapping</description>
    <package>com.programmeren4.turnahead.server.jpa</package>
    <entity class="com.programmeren4.turnahead.server.jpa.Character" name="Character">
        <table name="CHARACTER"/>
        <attributes>
            <id name="id">
                <generated-value strategy="TABLE"/>
            </id>
            <basic name="characterName">
                <column name="CHAR_NAME" length="100"/>
            </basic>
            <basic name="lastLogin">
                <column name="LAST_LOGIN" length="100"/>
            </basic>
             <basic name="registrationDate">
                <column name="REG_DATE" length="100"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>